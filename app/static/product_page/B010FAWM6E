    P.when("A", "load").execute("aod-assets-loaded", function(A){
        function logAssetsNotLoaded() {
            if (window.ueLogError) {
                var customError = { message: 'Failed to load AOD assets for WDG: baby_product_display_on_website, Device: web' };
