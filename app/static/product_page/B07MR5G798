    };

    vseClientAssets.setClientPrefix('rvs', options);
    
        A.trigger('vse:ns:player:rvs:init', 'rvs-player-942d2000-b16f-4f05-ba74-ef97363e289d-player-ps');
    
    
        A.trigger('vse:ns:metadata:rvs:init', 'rvs-video-information-a4dc7f27-6669-41df-89ae-1e58b3a74842-ps');
        
            A.trigger('vse:ns:socialShare:rvs:init', 'rvs-social-share-a4dc7f27-6669-41df-89ae-1e58b3a74842-ps');
        
    
    
        A.trigger('vse:ns:ingressCarousel:rvs:init', 'rvs-related-videos-914ae052-de50-4b65-881c-96a55835cd23-related-videos-ps', vwdpStartTime);
    
    });
}));</script>


