  <script type="text/javascript">
    P.when('A', 'a-modal', 'cf').execute(function mapleLinkModal (A, modal) {
  var $ = A.$;

  $(document).delegate('#maple-banner-modal-trigger', 'click', function handleClick (event) {
    event.preventDefault();

    var $trigger = $('#maple-banner-modal-trigger');
    var inlineContent = $('#maple-banner-modal-content').html();
    var modalHeader = '';
    var modalPadding = 'medium';
    var modalWidth = '700';
    var hideHeader = true;
    if (modalHeader) {
      hideHeader = false;
    }
    var options = {
      'header': modalHeader,
      'hideHeader': hideHeader,
      'inlineContent': inlineContent,
      'padding': modalPadding,
      'width': parseInt(modalWidth)
    };
    var instance = modal.create($trigger, options);
    instance.show();
  });
});

  </script>
