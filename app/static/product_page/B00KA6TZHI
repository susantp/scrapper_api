                      weblabTreatmentMap: {"CROSS_BORDER_INTERSTITIAL_ES_US_340017":"C","CROSS_BORDER_INTERSTITIAL_EG_302080":"C","NARX_GOLDBOX_REDIRECTION_319969":"T1","CROSS_BORDER_INTERSTITIAL_MX_US_341718":"C","CBI_355055":"C","NARX_INTERSTITIAL_NEW_CX_372291":"C","MWEB_CROSS_BORDER_INTERSTITIAL_SG_366762":"C","MWEB_CROSS_BORDER_INTERSTITIAL_SE_366766":"T1","MWEB_CROSS_BORDER_INTERSTITIAL_SA_366767":"T1","MWEB_CROSS_BORDER_INTERSTITIAL_PL_366768":"T1","MWEB_CROSS_BORDER_INTERSTITIAL_NL_366769":"T1","MWEB_CROSS_BORDER_INTERSTITIAL_AU_366773":"T1","MWEB_CROSS_BORDER_INTERSTITIAL_AE_366771":"T1","MWEB_CROSS_BORDER_INTERSTITIAL_EG_387234":"C"}
                  }),
                  contentType: "application/json",
                  dataType: "html",
                  success: function(data) {
                      if (data) {
                          $('body').append(data);
                      }
                  }
              });
      });
  </script>




